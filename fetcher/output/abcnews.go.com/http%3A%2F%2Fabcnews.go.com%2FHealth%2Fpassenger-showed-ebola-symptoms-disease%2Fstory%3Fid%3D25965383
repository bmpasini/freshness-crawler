$("#nav_top .share_group").css('visibility','visible');
}, 2500);
$('.short_nav').waypoint('sticky', {
wrapper: '<div class="short_nav_wrapper" />',
stuckClass: 'nav_stuck',
offset:20,
handler: function(dir) {
if(dir==='down'){
$('.nav_stuck').animate({top:'0'}, 250, 'linear', function() {
//done
});
} else {
$('.short_nav').css("top","-60px");
}
}
});
