<!--widget[ABTop_1_spev8_24808980_subindex]-->
<script type="text/javascript">
// <![CDATA[
var spev8 = new Object();
spev8.videoPlaying = false;
spev8.videoPlayingID = false;
spev8.videoHasPlayed = false;
spev8.currPTLink = false;
spev8.state = 'loading';
blockLoadFrame = false;
playerLoaded = false;
spev8.loadFrame = function(mediaId, nodeId, validVideo) {
if(playerLoaded == true){
this.id = mediaId;
this.node = nodeId;
this.video = validVideo;
this.showItem(nodeId);
if(this.video){
if((this.id==this.videoPlayingID) && (!spev8.videoHasPlayed)) {//____________________________ resume video already in player
if(!blockLoadFrame){
this.quickHideImage();
this.showPlayer();
}
amp.play()
this.videoPlaying = true;
} else {//______________________________________________________________________ new video request
if(spev8.state == "play"){
amp.pause();
blockLoadFrame = false;
}
}
} else {
if(this.videoPlaying) {
amp.pause();
}
this.videoPlaying = false;
blockLoadFrame = false;
}
} else {
this.id = mediaId;
this.showItem(nodeId);
}
}
spev8.previewLinkOpen = function(id,isVideo){
if(spev8.currPTLink){
if(ptObjId == '#s8thlink-'+id){
if(isVideo){
if(!this.videoPlaying) {
blockLoadFrame = true;
spev8.loadListOnclick(id);
}
}else{
location.href = spev8.currPTLink.href;
blockLoadFrame = false;
}
}
}
ptObjId = '#s8thlink-'+id;
spev8.currPTLink = document.getElementById('s8thlink-'+id);
}
spev8.loadListOnclick = function(id){
spev8.id = id;
spev8.loadList(null, id, true, true);
}
spev8.showItem = function(nodeId) {
metaPlaylist.gotoItem('spev8-'+nodeId);
$('div.wi-container').removeClass('active');
$('div#video-'+this.id).addClass('active');
if(this.id!=this.videoPlayingID) {
//setTimeout(spev8.hidePlayerShowImage, 300);
spev8.hidePlayerShowImage();
}
//__________________________________________________________ start onclick event listener to handle preview layer clicks
spev8.previewLinkOpen(this.id, this.video);
}
spev8.hidePlayerShowImage = function() {
document.getElementById('s8-image-container').style.display = 'block';
document.getElementById('s8-image-container').style.zIndex = '10000';
if(document.getElementById('spev8-MediaPlayer')) {
document.getElementById('spev8-MediaPlayer').style.zIndex = '0';
document.getElementById('spev8-MediaPlayer').style.top = '-1000px';
}
}
spev8.loadList = function() {
pc.playVideoByCID({id:this.id});
if (this.videoPlayingID == false) {
amp.play();
}
spev8.hideImageShowPlayer();
this.videoPlayingID = this.id;
this.videoPlaying = true;
this.videoHasPlayed = false;
}
spev8.hideImageShowPlayer = function() {
//setTimeout(this.imageFadeOut, 1100);
//setTimeout(this.showPlayer, 1100);
this.imageFadeOut();
this.showPlayer();
}
spev8.imageFadeOut = function() {
$('#s8-image-container').fadeOut(100, function() {
// Animation complete
});
}
spev8.showPlayer = function() {
document.getElementById('spev8-MediaPlayer').style.zIndex = '10000';
document.getElementById('spev8-MediaPlayer').style.top = '10px';
}
spev8.quickHideImage = function() {
document.getElementById('s8-image-container').style.display = 'none';
}
spev8.removeAdOverlay = function() {
if(window.spev8Overlay.style.display=='block') {
window.spev8Overlay.style.display = 'none';
}
}
spev8.showAdOverlay = function() {
if(window.spev8Overlay){
window.spev8Overlay.style.display='block';
}
}
spev8.ready = function() {
document.getElementById('spev8-MediaPlayer').style.zIndex = '0';
document.getElementById('spev8-MediaPlayer').style.top = '-1000px';
playerLoaded = true;
spev8.state = "ready";
spev8.removeAdOverlay();
}
spev8.reset = function() {
this.isVideoPlaying = false;
if(window.spev8ImageContainer){//show image on top of player when video ends
spev8.hidePlayerShowImage();
spev8.videoHasPlayed = true;
spev8.videoPlaying = false;
blockLoadFrame = false;
}
}
// ]]>
</script>
