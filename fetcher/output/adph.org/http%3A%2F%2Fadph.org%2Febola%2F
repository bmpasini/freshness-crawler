	function popup(URL,name,toolbar,scrollbars,location,statusbar,menubar,resizable,width,height,left,top)
	{	
		var popwin;
		var params = 'toolbar=' + toolbar + 
					 ',scrollbars=' + scrollbars + 
					 ',location=' + location + 
					 ',statusbar=' + statusbar + 
					 ',menubar=' + menubar + 
					 ',resizable=' + resizable + 
					 ',width=' + width + 
					 ',height=' + height + 
					 ',left=' + left + 
					 ',top=' + top;
		popwin = window.open(URL, name, params);
		popwin.focus();
	}
